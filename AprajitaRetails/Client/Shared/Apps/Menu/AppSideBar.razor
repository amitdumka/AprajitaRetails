                </SideBarMenuItem>
                <SideBarMenuItem Icon="far fa-circle" Link="contacts">
                    Contacts
                </SideBarMenuItem>
                <SideBarMenuItem Icon="far fa-circle" Link="salesmen">
                    Salesmen
                </SideBarMenuItem>
            </Items>
        </SideBarTreeView>
