                        </SideBarMenuItem>
                    </Authorized>
                    <NotAuthorized>
                        <SideBarMenuItem Icon="far fa-circle" Link="login">
                            Login
                        </SideBarMenuItem>
                        <SideBarMenuItem Icon="far fa-circle" Link="register">
                            Register
                        </SideBarMenuItem>
                    </NotAuthorized>
                </AuthorizeView>
            </Items>
